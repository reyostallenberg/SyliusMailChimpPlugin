{
    "name": "bitbag/mailchimp-plugin",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "MailChimp plugin for Sylius.",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^7.4 || ^8.0",
        "sylius/sylius": "~1.9.0 || ~1.10.0 || ~1.11.0",
        "drewm/mailchimp-api": "^v2.5.4",
        "ext-json": "*",
        "vimeo/psalm": "^4.12",
        "composer/xdebug-handler": "^2.0",
        "bitbag/coding-standard": "^1.0"
    },
    "require-dev": {
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BitBag\\SyliusMailChimpPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true,
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
